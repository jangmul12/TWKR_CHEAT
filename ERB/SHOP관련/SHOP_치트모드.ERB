@CHEAT_BUTTON_200(BTN, CAR)
#DIM BTN
#DIM CAR

IF BTN == 201
	CALL CHAR_EDITOR(MASTER,2)
ELSEIF BTN == 202
	CALL CHEAT_DICK_TYPE(MASTER)
ELSEIF BTN == 203
	IF TALENT:MASTER:농후정액 == 1
		TALENT:MASTER:농후정액 = 0
	ELSE
		TALENT:MASTER:농후정액 = 1
	ENDIF
ELSEIF BTN == 204
	CALL BASE_CUSTOM_MODE(MASTER)
ELSEIF BTN == 205
	FOR LOCAL,0,CHARANUM
		BASE:LOCAL:체력 = MAXBASE:LOCAL:체력
		BASE:LOCAL:기력 = MAXBASE:LOCAL:기력
	NEXT
ELSEIF BTN == 206
	PRINTL 전 캐릭터의 욕구불만도를 변경합니다.
	PRINTL 0 ~ 1000이내의 숫자를 수동으로 입력 가능.(그 외의 숫자를 입력하면 취소됩니다.)
	PRINTL
	PRINTBUTTON "[뒤로]", 9999
	PRINT 　
	PRINTBUTTON "[   0]", 0
	PRINT 　
	PRINTBUTTON "[ 300]", 300
	PRINT 　
	PRINTBUTTON "[ 500]", 500
	PRINT 　
	PRINTBUTTON "[ 700]", 700
	PRINT 　
	PRINTBUTTON "[1000]", 1000
	PRINTL
	INPUT
	CLEARLINE 1
	IF RESULT >= 0 && RESULT <= 1000
		FOR LOCAL, 1, CHARANUM
			CFLAG:LOCAL:욕구불만도 = RESULT
		NEXT
	ELSE
		RETURN
	ENDIF
ELSEIF BTN == 207
	CLEARLINE LINECOUNT
	DRAWLINE
	PRINTL
	PRINTL 아이가 있는 캐릭터
	PRINTL
	LOCAL:1 = 0
	FOR LOCAL, 101, CHARANUM + 100
		SIF !CFLAG:(LOCAL-100):아이인원수
			CONTINUE
		LOCAL:1 ++
		PRINTBUTTON @" [{LOCAL}] %CALLNAME:((LOCAL)-100),12,LEFT%", LOCAL - 100
		SIF LOCAL:1 % 4 == 0
			PRINTL
	NEXT
	PRINTL
	IF LOCAL:1 > 0
		PRINTL
		PRINTL [999] 뒤로
		PRINTL
		INPUT
		CLEARLINE 1
		IF RESULT == 999
			RETURN
		ELSE
			CALL CHEAT_CHILD_LIST(RESULT)
		ENDIF
	ELSE
		PRINTW 아이가 있는 캐릭터가 없습니다.
	ENDIF
ENDIF

@CHEAT_BUTTON_300(BTN, CAR)
#DIM BTN
#DIM CAR
IF BTN == 301
	CALL CHEAT_CHAR_SELECT
ELSEIF BTN == 302
	CALL CHAR_EDITOR(CAR,2)
ELSEIF BTN == 303
	CALL BASE_CUSTOM_MODE(CAR)
ELSEIF BTN == 304
	CLEARLINE LINECOUNT
	CALL PRINT_STATE_TALENT(CAR)
	PRINTL -------------------------------------------------------------------------------------
	LOCAL:1 = 0
	FOR LOCAL, 0, 157
		SETCOLOR 150, 150, 150
		IF CUSTOM_선택할수없는소질(LOCAL, NO:CAR) == 1
			CONTINUE
		ENDIF
		SELECTCASE TALENTNAME:LOCAL
		CASE ""
			CONTINUE
		CASE GET_TALENTNAME(LOCAL, 1)
			SIF TALENT:CAR:LOCAL
				SETCOLOR C_AQUA
			LOCALS = [{LOCAL, 3}]%TALENTNAME:LOCAL%
		CASE "비동정"
			SIF !TALENT:CAR:LOCAL
				SETCOLOR C_AQUA
			LOCALS = [{LOCAL, 3}]동정
		;양적 소질
		CASEELSE
			LOCALS =  [{LOCAL, 3}]%TALENTNAME:LOCAL%
			IF STRLENS(GET_TALENTNAME(LOCAL, TALENT:CAR:LOCAL))
				LOCALS:1 = 【%GET_TALENTNAME(LOCAL, TALENT:CAR:LOCAL)%】
				SETCOLOR C_AQUA
			ELSEIF TALENTNAME:LOCAL == "성별"
				LOCALS:1 = 【여자】
				SETCOLOR C_AQUA
			ELSE
				LOCALS:1 = 【－】
			ENDIF
			LOCALS += LOCALS:1
		ENDSELECT
		SIF LOCAL:1 % 3 == 0
			PRINTL 
		PRINTFORM %LOCALS, 30, LEFT%
		LOCAL:1 ++
	NEXT
	RESETCOLOR
	PRINTL
	PRINTL
	PRINTFORM [999] 뒤로
	INPUT
	CLEARLINE 1
	
	IF RESULT == 999
		RETURN
	ELSE
		SELECTCASE TALENTNAME:RESULT
		CASE GET_TALENTNAME(RESULT, 1)
			IF TALENT:CAR:RESULT == 1
				TALENT:CAR:RESULT = 0
			ELSE
				TALENT:CAR:RESULT = 1
			ENDIF
		;양적 소질
		CASEELSE
			IF TALENTNAME:RESULT == "성별"
				IF TALENT:CAR:RESULT == 1
					TALENT:CAR:RESULT = 2
				ELSEIF TALENT:CAR:RESULT == 2
					TALENT:CAR:RESULT = 3
				ELSEIF TALENT:CAR:RESULT == 3
					TALENT:CAR:RESULT = 1
				ENDIF
			ELSE
				TALENT:CAR:RESULT += 1
				IF STRLENS(GET_TALENTNAME(RESULT, TALENT:CAR:RESULT)) == 0 && TALENT:CAR:RESULT != 0
					FOR LOCAL:10, -3, 5
						TALENT:CAR:RESULT = LOCAL:10
						IF STRLENS(GET_TALENTNAME(RESULT, TALENT:CAR:RESULT)) != 0
							BREAK
						ENDIF
					NEXT
				ENDIF
			ENDIF
		ENDSELECT
	ENDIF
	RESTART
ELSEIF BTN == 305
	CLEARLINE LINECOUNT
	PRINTL □ 능력 □---------------------------------------------------------------------------
	IF !TFLAG:능력표시
		LOCAL:1 = 0
		FOR LOCAL, 0, 50
			SIF !STRLENS(ABLNAME:LOCAL)
				CONTINUE
			SIF LOCAL > 50 && CAR != MASTER
				CONTINUE
			;동성애 관계
			SIF GROUPMATCH(ABLNAME:LOCAL, "레즈끼", "레즈중독") && !GETBIT(TALENT:CAR:2, 0)
				CONTINUE
			SIF GROUPMATCH(ABLNAME:LOCAL, "호모끼", "게이중독") && GETBIT(TALENT:CAR:2, 0)
				CONTINUE
			PRINTFORM %ABLNAME:LOCAL,12%:
			IF LOCAL < 40
				CALL PRINT_RANK(ABL:CAR:LOCAL, "MAX_99")
			ELSE
				CALL PRINT_RANK(ABL:CAR:LOCAL, "MAX_6")
			ENDIF
			PRINTFORM  {ABL:CAR:LOCAL, 5, LEFT} 
			LOCAL:1 ++
			SIF LOCAL:1 % 4 == 0
				PRINTL 
		NEXT
		PRINTL
	ENDIF
	PRINTL -------------------------------------------------------------------------------------
	LOCAL:1 = 0
	FOR LOCAL, 0,50
		SIF !STRLENS(ABLNAME:LOCAL)
			CONTINUE
		;동성애 관계
		IF !(TALENT:CAR:2 & 1) && (LOCAL == 17 || LOCAL == 32)
			CONTINUE
		ELSEIF TALENT:CAR:2 & 1 && (LOCAL == 18 || LOCAL == 33)
			CONTINUE
		ENDIF
		;중독도 없음
		SIF LOCAL >= 30
			CONTINUE
		PRINTFORM %ABLNAME:LOCAL,12%:{ABL:CAR:LOCAL, 5, LEFT} 
		IF ABL:CAR:LOCAL != 0
			PRINTBUTTON "[－]", LOCAL * 2
		ELSE
			SETCOLOR 0x969696
			PRINT [－]
			RESETCOLOR
		ENDIF
		IF ABL:CAR:LOCAL != 50
			PRINTBUTTON "[＋]", LOCAL * 2 + 1
		ELSE
			SETCOLOR 0x969696
			PRINT [＋]
			RESETCOLOR
		ENDIF
		LOCAL:1 ++
		SIF LOCAL:1 % 3 == 0
			PRINTL 
	NEXT
	PRINTL
	PRINTL
	PRINTFORM [999] 뒤로
	INPUT
	CLEARLINE 1
	IF RESULT == 999
		RETURN
	ELSE
		LOCAL:3 = RESULT / 2
		LOCAL:4 = RESULT % 2
		IF LOCAL:4 == 0
			ABL:CAR:(LOCAL:3) -= 1
		ELSEIF LOCAL:4 == 1
			ABL:CAR:(LOCAL:3) += 1
		ENDIF
	ENDIF
	RESTART
ELSEIF BTN == 306
	CLEARLINE LINECOUNT
	CALL PRINT_STATE_EXP(CAR)
	PRINTL -------------------------------------------------------------------------------------
	LOCAL:1 = 0
	FOR LOCAL,0,100
		SIF EXPNAME:LOCAL == ""
			CONTINUE
		PRINTFORM   [{LOCAL,2}] - %EXPNAME:LOCAL,16%:{EXP:CAR:LOCAL,5,LEFT} 
		LOCAL:1 ++
		SIF LOCAL:1 % 3 == 0
			PRINTL 
	NEXT
	PRINTL
	PRINTL
	PRINTFORM [999] 뒤로
	PRINTL
	INPUT
	CLEARLINE 1
	IF RESULT == 999
		RETURN
	ELSE
		LOCAL:1 = RESULT
		PRINTL
		PRINTL 경험을 입력하세요. 수동으로 입력 가능합니다.
		PRINTL
		PRINTBUTTON "[ 캔슬 ]",-4
		PRINTBUTTON "[  10＋]",-1
		PRINTBUTTON "[ 100＋]",-2
		PRINTBUTTON "[1000＋]",-3
		INPUT
		CLEARLINE 1
		IF RESULT == -1
			EXP:CAR:(LOCAL:1) += 10
		ELSEIF RESULT == -2
			EXP:CAR:(LOCAL:1) += 100
		ELSEIF RESULT == -3
			EXP:CAR:(LOCAL:1) += 1000
		ELSEIF RESULT <= -4
			RESTART
		ELSE
			EXP:CAR:(LOCAL:1) = RESULT
		ENDIF
		RESTART
	ENDIF
	RESTART
ELSEIF BTN == 307
	CALL CHEAT_SEXTECH(CAR)
ELSEIF BTN == 308
	CALL CHEAT_RELATIONS(CAR)
ELSEIF BTN == 309
	CALL CHEAT_MARK(CAR)
ELSEIF BTN == 310
	SELECTCASE CFLAG:CAR:생리주기
	CASE 0 TO 2
		CFLAG:CAR:생리주기 = 3
	CASE 3 TO 5
		CFLAG:CAR:생리주기 = 6
	CASE 6
		CFLAG:CAR:생리주기 = 7
	CASE 7
		CFLAG:CAR:생리주기 = 8
